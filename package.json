{
  "name": "lazygit-vscode",
  "displayName": "LazyGit for VSCode",
  "description": "Use LazyGit in a VSCode window",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tom-pollak/lazygit-vscode"
  },
  "categories": [],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lazygit-vscode.toggleLazyGit",
        "title": "Toggle LazyGit"
      }
    ],
    "keybindings": [
      {
        "command": "lazygit-vscode.toggleLazyGit",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
